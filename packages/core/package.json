  "description": "Unstyled/headless components for Solid-Bootstrap",
  "homepage": "https://github.com/solid-libs/solid-bootstrap/tree/master#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solid-libs/solid-bootstrap.git#master"
  },
